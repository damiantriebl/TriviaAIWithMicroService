{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Iniciar el chatPRoxy",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\chatProxy\\src\\server.ts",
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ]
        },
        {
            "type": "node-terminal",
            "name": "Ejecutar el script: dev",
            "request": "launch",
            "command": "npm run dev",
            "cwd": "${workspaceFolder}\\Frontend"
        }
    ]
}